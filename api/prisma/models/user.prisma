model User {
  id              Int        @id @default(autoincrement())
  name            String
  email           String     @unique
  password        String
  image           String?
  email_verified_at DateTime?
  dob             DateTime?   
  phone           String? 
  remember_token   String?
  is_admin         Int         @default(0)
  status          Int         @default(1)
  created_at      DateTime    @default(now())
  updated_at      DateTime    @updatedAt

  @@map("users")
}
